{
  "name": "box-developer-changelog",
  "version": "1.0.0",
  "description": "Box Developer Changelog",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/box/box-developer-changelog.git"
  },
  "author": "Box <developer@box.com>",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build": "concurrently 'yarn:build:*'",
    "build:entries": "babel-node -e 'require(\"./src\").writeChangelog()'",
    "clean": "rm -rf ./compiled",
    "clean:all": "rm -rf ./.sources ./compiled",
    "lint:alex": "alex content/ -q",
    "lint": "concurrently 'yarn lint:links' 'yarn lint:markdown' 'yarn lint:spelling' 'yarn lint:alex'",
    "lint:links": "jest ./src/tests/*",
    "lint:markdown": "markdownlint -c markdownlintrc.json ./content --fix",
    "lint:spelling": "yarn lint:spelling:interactive -r",
    "lint:spelling:interactive": "mdspell 'content/**/*.md' README.md CONTRIBUTING.md -a -n --en-us -d dictionaries/en_US",
    "pull": "babel-node -e 'require(\"./src/scripts/pull.js\").pull()'",
    "prebuild": "yarn clean",
    "prelint": "yarn pull",
    "test": "jest ./tests/* --coverage --collectCoverageFrom=src/**/*.js --coverageReporters=html --coverageReporters=text",
    "import:release": "babel-node -e 'let Importer = require(\"./src/importer/\").default; new Importer().importRelease({ destination: \"./content\" })'"
  },
  "dependencies": {
    "@babel/node": "^7.10.3",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "@babel/runtime": "^7.10.3",
    "alex": "^8.1.1",
    "axios": "^0.19.2",
    "babel-jest": "^26.1.0",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "jest": "^26.0.1",
    "js-yaml": "^3.14.0",
    "markdown-spellcheck": "^1.3.1",
    "markdownlint": "^0.20.4",
    "markdownlint-cli": "^0.23.1",
    "markdownlint-rule-helpers": "^0.11.0",
    "nock": "^12.0.3",
    "remark": "^12.0.0",
    "remark-github": "^9.0.1",
    "remark-reference-links": "^5.0.0",
    "sha1-from-file": "^2.0.1",
    "strip-indent": "^3.0.0",
    "uslug": "^1.0.4"
  }
}
